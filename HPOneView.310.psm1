			{

				'StoreVirtual'
				{

					$_newVolume = NewObject -StoreVirtualStorageVolume

				}

				'StoreServ'
				{

					$_newVolume = NewObject -StoreServStorageVolume

				}

			}

			$_Family = $VolumeTemplate.family

