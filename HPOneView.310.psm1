				}

				#Get the list of unmanaged and managed pools in the managed domain
				Try
				{
					
					$_Results = Send-HPOVRequest ("{0}?filter=name EQ '{1}'" -f $_storagesystem.storagePoolsUri, $_pool) -Hostname $_appliance.Name
