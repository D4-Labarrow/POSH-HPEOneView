						$PSCmdlet.ThrowTerminatingError($ErrorRecord)

					}

					"[{0}] Setting snapshot storage pool: {1}" -f $MyInvocation.InvocationName.ToString().ToUpper(), $_SnapShotStoragePool.uri | Write-Verbose

