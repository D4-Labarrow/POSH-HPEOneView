						[void]$_ColStatus.Add($_resp)

					}

					Catch
					{

						$PSCmdlet.ThrowTerminatingError($_)

					}

				}

				else 
				{

					Write-Verbose "[$($MyInvocation.InvocationName.ToString().ToUpper())] No Pending update found"

				}

			}

		}

	}

	End 
	{ 
	
		Return $_ColStatus

	}

