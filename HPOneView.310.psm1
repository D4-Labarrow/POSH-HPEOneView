			{

				$PSCmdlet.ThrowTerminatingError($_)

			}

			switch ($_RackItemObject.category)
			{

				'server-hardware'
				{

					$_Model = $_RackItemObject.model
					[int]$_UHeight = $_RackItemObject.formFactor.Replace('U',$null)

