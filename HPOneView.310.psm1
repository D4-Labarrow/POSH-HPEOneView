					$_ZoneObject.State               = $_MemberZone.state
					$_ZoneObject.Status              = $_MemberZone.status
					$_ZoneObject.ManagedSan          = $_MemberZone.sanName
					$_ZoneObject.Created             = $_MemberZone.created
