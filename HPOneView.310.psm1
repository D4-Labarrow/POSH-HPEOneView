						$_Alias.WWN = $_AliasMember.members

						[void]$_ZoneObject.Members.Add($_Alias)

					}

					[void]$_FCZoneCollection.Add($_ZoneObject)

				}

			}

		}

	}

	End
	{

		Return $_FCZoneCollection

	}

