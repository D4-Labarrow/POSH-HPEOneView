function Get-HPOVUser 
{

	# .ExternalHelp HPOneView.310.psm1-help.xml

	[CmdletBinding ()]
	Param 
	(

		[Parameter (Mandatory = $false)]
		[ValidateNotNullorEmpty()]
		[string]$Name,
		
		[Parameter (Mandatory = $false)]
