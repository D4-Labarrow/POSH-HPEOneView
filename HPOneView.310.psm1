		[Parameter (Mandatory = $False, ParameterSetName = "default")]
		[int]$EgressDot1pValue,

		[Parameter (Mandatory = $False, ParameterSetName = "default")]
		[System.Collections.ArrayList]$IngressDot1pClassMapping,
		
		[Parameter (Mandatory = $False, ParameterSetName = "default")]
		[System.Collections.ArrayList]$IngressDscpClassMapping,

		[Parameter (Mandatory = $False, ParameterSetName = "default")]
		[switch]$RealTime,

		[Parameter (Mandatory = $False, ParameterSetName = "default")]
