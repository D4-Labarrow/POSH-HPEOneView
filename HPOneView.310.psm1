function New-HPOVSupportDump 
{

	# .ExternalHelp HPOneView.310.psm1-help.xml

	[CmdletBinding (DefaultParameterSetName = "values")]
	Param 
	(

		[Parameter (Mandatory = $false,ValueFromPipeline = $false, ParameterSetName = "values")]
