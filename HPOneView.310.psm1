					 @{Expression={($_.interconnectName -split ",")[1]};Label="Interconnect"},		         
					 @{Expression={$_.networkInterface};Label="Interface"},
					 @{Expression={$_.macAddress};Label="Address"},
					 @{Expression={$_.entryType};Label="Type"},
