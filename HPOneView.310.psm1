function Get-HPOVStorageSystem 
{

	# .ExternalHelp HPOneView.310.psm1-help.xml

	[CmdletBinding (DefaultParameterSetName = "Name")]
	Param 
	(

		[Parameter (Mandatory = $false, ParameterSetName = "Name")]
		[ValidateNotNullOrEmpty()]
		[Alias ('SystemName')]
		[string]$Name,

